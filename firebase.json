{
  "apphosting": {
    "backendId": "c2-backend-dev",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run":{
          "serviceId": "c2-backend-dev",
          "region": "europe-west4"
        }
      }
    ]
  }
}
